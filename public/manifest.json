{
  "manifest_version": 2,
  "short_name": "Extended",
  "name": "Extended",
  "description": "chat extension that allows users to communicate on the same domain",
  "version": "0.0.1",
  "background": {
    "page": "background-page.html"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Extended"
  },
  "content_security_policy": "script-src 'self' 'sha256-dNJNeOnc8+lJDlAXjFIOEf2t8W4N+mghvZDYhPFate0='; object-src 'self'"
}
